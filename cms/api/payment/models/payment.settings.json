{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "name": "payment"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "middle_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "dob": {
      "type": "date"
    },
    "card_holder_name": {
      "type": "string"
    },
    "card_type": {
      "type": "enumeration",
      "enum": [
        "Visa",
        "Master",
        "AmericanExpress",
        "Discover"
      ]
    },
    "cvv": {
      "type": "string"
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "cityname": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "alternate_phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  }
}
